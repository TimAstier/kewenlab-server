    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "forest-express-sequelize": "^1.3.0",
