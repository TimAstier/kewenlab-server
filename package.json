    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel": "^6.5.2",
